main :: IO()
soma :: Integer->Integer->Integer->String
soma a b c
    |a+b<c = "a+b e menor que c"
    |a+b>c = "a+b e maior que c"
    |a+b==c = "a+b e igual a c"

main = do

    putStrLn("Digite valor de a")
    a<-readLn
    putStrLn("Digite valor de b")
    b<-readLn
    putStrLn("Digite valor de c")
    c<-readLn
   
    putStrLn("\n"++show(soma a b c))
